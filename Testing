const oldPointStructure = {
   1: ['A', 'E', 'I', 'O', 'U', 'L', 'N', 'R', 'S', 'T'],
   2: ['D', 'G'],
   3: ['B', 'C', 'M', 'P'],
   4: ['F', 'H', 'V', 'W', 'Y'],
   5: ['K'],
   8: ['J', 'X'],
   10: ['Q', 'Z']
};

const transform = (obj) => {
    let newObj = {};
    for (let item in obj) {
        for (let i=0; i < obj[item].length; i++) {
            newObj[obj[item][i]] = item
        }
    }
    return newObj
}

const newPointStructure = transform(oldPointStructure);
console.log(newPointStructure)
